                </a>
            </div>

            <div class="col-xs-9 col-sm-9 col-md-9" style="">
                <a href="">

                    <div class="text1" style="display:block; white-space: nowrap;">@需求编号</div>
                </a>
            </div>

        </div>


    </div>



    @foreach (打手接单视图表1 aa in Model.ToList())
    {




        <div class="alert alert-info " role="alert" data-toggle="popover" data-placement="top"
             title="打手信息" data-container="body" data-content="测试">

            <div class="row">
                <div class="  col-xs-10 col-sm-10 col-md-10">

                    <div class="row">
                        <div class="col-xs-4 col-sm-4 col-md-4">打手qq</div>
                        <div class="col-xs-4 col-sm-4 col-md-4">报价</div>
                        <div class="col-xs-4 col-sm-4 col-md-4">需要时间</div>
                        <div class="col-xs-4 col-sm-4 col-md-4">@aa.隐藏打手qq</div>
                        <div class="col-xs-4 col-sm-4 col-md-4">@aa.报价</div>
                        <div class="col-xs-4 col-sm-4 col-md-4">@aa.预计花费时间</div>
                    </div>


                </div>






                <div class="col-xs-2 col-sm-2 col-md-2 " style="margin-top:18px; ">
                    <a href=@aa.直播或主页链接><p type="button" class="btn btn-warning">  直播间</p> </a>
                </div>


                <div class="col-xs-3 col-sm-3 col-md-3">熟练度</div>
                <div class="col-xs-3 col-sm-3 col-md-3">
                    @aa.打手信息
                </div>
                <div class="col-xs-3 col-sm-3 col-md-3">安全系数</div>




                <div class="col-xs-3 col-sm-3 col-md-3">
                    @aa.安全系数
                </div>

                <div class="col-xs-6 col-sm-6 col-md-6">代肝内容</div>

                <div class="col-xs-9 col-sm-9 col-md-9">
                    @aa.业务
                </div>

            </div>

        </div>
        <script>
            $('div').popover();
        </script>

    }


    <form action="/yuanshen/报价" method="post" name="form1">


        <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal"
                                aria-hidden="true">
                            ×
                        </button>
                        <h4 class="modal-title" id="myModalLabel">
                            报价
                        </h4>
                    </div>
                    <div class="modal-body">
                        请填写报价信息
                        <input type="text" name="需求编号" style="display:none" class="form-control" placeholder="请输入需求编号" value=@ViewBag.需求编号>
                        <input type="text" name="打手qq" class="form-control" placeholder="请输入qq">
                        <input type="text" name="报价" class="form-control" placeholder="请输入报价（元）">
                        <input type="text" name="预计花费时间" class="form-control" placeholder="请输入预计花费时间（分）">
                        <input type="text" name="报价时间" style="display:none" id="date_info" class="form-control" placeholder="请输入报价时间">

                        <script>
                            $(document).ready(function () {
                                var time = new Date();
                                var day = ("0" + time.getDate()).slice(-2);
                                var month = ("0" + (time.getMonth() + 1)).slice(-2);
                                var m = ("0" + (time.getMinutes() + 1)).slice(-2);
                                var h = ("0" + (time.getHours())).slice(-2);
                                var s = ("0" + (time.getSeconds() + 1)).slice(-2);
                                var today = time.getFullYear() + "-" + (month) + "-" + (day) + " " + (h) + ":" + (m) + ":" + (s);
                                $('#date_info').val(today);
                            })
                        </script>
                    </div>



                    <div class="modal-footer">

                        <button type="button" class="btn btn-primary" onclick="form1.action= '/yuanshen/报价' ;form1.submit();">
                            确定
                        </button>
                        <button type="button" class="btn btn-default"
                                data-dismiss="modal">
                            取消
                        </button>


                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->



    </form>






    <div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"
                            aria-hidden="true">
                        ×
                    </button>
                    <h4 class="modal-title" id="myModalLabel">
                        qq
                    </h4>
                </div>
                <div class="modal-body">
                    请填写你的qq
                    <input type="text" id="输入qq" name="需求编号" class="form-control" placeholder="请输入需求编号" value="">



                </div>
                <div class="modal-title" id="在线状态"></div>


                <div class="modal-footer">

                    <button type="button" class="btn btn-primary" onclick="查询买家qq()">
                        确定
                    </button>
                    <button type="button" class="btn btn-default"
                            data-dismiss="modal">
                        取消
                    </button>


                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->





    <div class="modal fade" id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal"
                            aria-hidden="true">
                        ×
                    </button>
                    <h4 class="modal-title" id="myModalLabel">
                        qq
                    </h4>
                </div>
                <div class="modal-body">
                    请填写你的qq
                    <input type="text" id="输入打手qq" name="需求编号" class="form-control" placeholder="请输入需求编号" value="">



                </div>
                <div class="modal-title" id="在线状态1"></div>


                <div class="modal-footer">

                    <button type="button" class="btn btn-primary" onclick="打手查询中标qq()">
                        确定
                    </button>
                    <button type="button" class="btn btn-default"
                            data-dismiss="modal">
                        取消
                    </button>


                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->



    <div>&nbsp;</div>
