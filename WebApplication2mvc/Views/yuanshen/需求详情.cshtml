@using WebApplication2mvc.Models
@model  List<打手接单视图表1>
@{
    Layout = null;
}

<!doctype html>
<html lang="zh-CN">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
    <title>原神云代练 原神代肝</title>

    <!-- Bootstrap
    <link rel="stylesheet" href="./css/bootstrap.min.css"  >


        <script src="https://cdn.jsdelivr.cn/npm/jquery@1.12.4/dist/jquery.min.js" ></script>

        <script src="./js/bootstrap.min.js"  ></script>   -->
    <!-- Bootstrap
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet">
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js"></script>
    -->


    <link rel="stylesheet" href="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script>
     @{
         string 需求编号 = "";
         string 名称 = "";
         int? 数量 = 0;
         decimal? 出价 = 0;
         string 游戏账号 = "";
         string qq = "";
         string 备注 = "";
         string 隐藏qq = "";
         string 隐藏打手qq = "";
         string 隐藏已选择打手qq = "";
         string 隐藏游戏账号 = "";

    }

    @foreach (打手接单视图表1 item in Model.ToList())
    {

        需求编号 = item.需求编号;
        名称 = item.名称;
        数量 = item.数量;
        出价 = item.出价;
        游戏账号 = item.游戏账号;
        qq = item.qq;
        备注 = item.备注;
        隐藏qq = item.隐藏qq;
        隐藏打手qq= item.隐藏打手qq;
        隐藏已选择打手qq = item.隐藏已选择的打手qq;
        隐藏游戏账号 = item.隐藏游戏账号;
    }

        @{

      string dingdanhao = "";
    dingdanhao = "/yuanshen/查询买家qq?q=" + qq;}




           function 查询买家qq() {
            var xmlhttp;
            if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
                xmlhttp = new XMLHttpRequest();
               }
               var dingdanhao = "/yuanshen/查询买家qq?q=" + document.getElementById("输入qq").value +  "&q1=@需求编号"   ;
            // alert(dingdanhao);
            xmlhttp.open("GET",  dingdanhao   , true);
            xmlhttp.send();

           // document.getElementById("在线状态").innerHTML = "bbbbbbbbbb";
            xmlhttp.onreadystatechange = function () {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {

                   // alert(xmlhttp.responseText);
                   // alert(3333);
                     if (xmlhttp.responseText == "0") {
                       // alert(11111);
                        document.getElementById("在线状态").innerHTML = "输入错误";
                         //  window.location = "/yuanshen/需求详情选择打手?q=@需求编号";
                    }
                    if (xmlhttp.responseText == "1") {
                       // alert(11111);
                       // document.getElementById("在线状态").innerHTML = "在线";
                       window.location = "/yuanshen/需求详情选择打手?q=@需求编号";
                    }
                }
            }
          //  window.location = "/yuanshen/index";
        }





         function 打手查询中标qq() {
            var xmlhttp;
            if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
                xmlhttp = new XMLHttpRequest();
               }
             var dingdanhao = "/yuanshen/打手查询中标qq?q=" + document.getElementById("输入打手qq").value +  "&q1=@需求编号"   ;
            // alert(dingdanhao);
            xmlhttp.open("GET",  dingdanhao   , true);
            xmlhttp.send();

           // document.getElementById("在线状态").innerHTML = "bbbbbbbbbb";
            xmlhttp.onreadystatechange = function () {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {

                   // alert(xmlhttp.responseText);
                     //alert(3333);
                     if (xmlhttp.responseText == "0") {
                       // alert(11111);
                        document.getElementById("在线状态1").innerHTML = "输入错误";
                         //  window.location = "/yuanshen/需求详情选择打手?q=@需求编号";
                    }
                    if (xmlhttp.responseText == "1") {
                       // alert(11111);
                       // document.getElementById("在线状态").innerHTML = "在线";
                       window.location = "/yuanshen/需求详情查看买家?q=@需求编号";
                    }
                }
            }
          //  window.location = "/yuanshen/index";
        }




    </script>
    <style>




        a {
            color: #000000;
            text-decoration: none;
        }

