    <script>
     @{
         string 需求编号 = "";
         string 名称 = "";
         int? 数量 = 0;
         decimal? 出价 = 0;
         string 游戏账号 = "";
         string qq = "";
         string 备注 = "";
         string 隐藏qq = "";
         string 隐藏打手qq = "";
         string 隐藏已选择打手qq = "";
         string 已选择打手qq = "";

    }

    @foreach (打手接单视图表1 item in Model.ToList())
    {

        需求编号 = item.需求编号;
        名称 = item.名称;
        数量 = item.数量;
        出价 = item.出价;
        游戏账号 = item.游戏账号;
        qq = item.qq;
        备注 = item.备注;
        隐藏qq = item.隐藏qq;
        隐藏打手qq = item.隐藏打手qq;
        隐藏已选择打手qq = item.隐藏已选择的打手qq;
        已选择打手qq = item.选择的打手qq;
    }

        @{

      string dingdanhao = "";
    dingdanhao = "/yuanshen/查询买家qq?q=" + qq;}


        function radio测试() {
            var value = "";
            var radio = document.getElementsByName("打手qqlength");
            for (var i = 0; i < radio.length; i++) {
              //  alert(radio.length);
               // alert(radio[i].value);
                if (radio[i].checked == true) {
                    if (radio[i].value == "on") { alert("onononon"); break;}
                   // alert(22222222);
                    value = radio[i].value;

                    break;
                }
            }
 }

        function 查询买家qq() {


            var value = "";
            var radio = document.getElementsByName("打手qqlength");
            for (var i = 0; i < radio.length; i++) {
                //  alert(radio.length);
                // alert(radio[i].value);
                if (radio[i].checked == true) {
                    if (radio[i].value == "on") { value = radio[i].value; break; }
                    // alert(22222222);
                    value = radio[i].value;

                    break;
                }
            }




            var xmlhttp;
            if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
                xmlhttp = new XMLHttpRequest();
               }
               var dingdanhao = "/yuanshen/确定打手?q=" + value +  "&q1=@需求编号"   ;
            // alert(dingdanhao);

            if (radio[i].value == "on") { } else { xmlhttp.open("GET", dingdanhao, true);}


            xmlhttp.send();

           // document.getElementById("在线状态").innerHTML = "bbbbbbbbbb";
            xmlhttp.onreadystatechange = function () {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {

                  //  alert(xmlhttp.responseText);
                  //  alert(3333);



                    document.getElementById("在线状态").innerHTML = xmlhttp.responseText;


                }
            }
          //  window.location = "/yuanshen/index";
        } </script>
