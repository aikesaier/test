@using WebApplication2mvc.Models
@model   List<需求表视图>
@{
    Layout = null;
}

<!doctype html>
<html lang="zh-CN">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <!-- 上述3个meta标签*必须*放在最前面，任何其他内容都*必须*跟随其后！ -->
    <title>原神云代练 原神代肝</title>

    <!-- Bootstrap
    <link rel="stylesheet" href="./css/bootstrap.min.css"  >


        <script src="https://cdn.jsdelivr.cn/npm/jquery@1.12.4/dist/jquery.min.js" ></script>

        <script src="./js/bootstrap.min.js"  ></script>   -->
    <!-- Bootstrap
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet">
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/js/bootstrap.bundle.min.js"></script>
    -->


    <link rel="stylesheet" href="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/css/bootstrap.min.css">
    <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdn.staticfile.org/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>

    <style>




        a {
            color: #000000;
            text-decoration: none;
        }

            a:hover {
                color: #000000;
                text-decoration: none;
            }



        img {
            width: 80px;
            height: 80px;
            padding: 10px;
            background-size: cover;
            border-radius: 25px;
        }





        .foot {
            justify-content: space-around;
            position: fixed;
            bottom: 0px;
        }
    </style>
</head>
<body>

    <div class="title" style="padding: 5px;font-size: 16px;  text-align:left"><p>需求列表</p></div>

    @foreach (需求表视图 item in Model.ToList())
    {



        <a href="/yuanshen/需求详情?q=@item.需求编号">
            <div class="alert alert-info alert-dismissible" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>



                <div class="row">

                    <div class="col-xs-12 col-sm-12 col-md-12" style=" ">


                        <div class="text1" style="display:block; white-space: nowrap;">@item.名称</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style=" text-align:center ">

                        数量

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1  " style="display:block; white-space: nowrap;">出价</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1  " style="display:block; white-space: nowrap;">游戏账号</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3 " style="  ">


                        <div class="text1  " style="display:block; white-space: nowrap;">QQ</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style="text-align: center;background-color: #dff0d8; ">

                        @item.数量

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style="background-color: #dff0d8; ">


                        <div class="text1 alert-success" style="display:block; white-space: nowrap;"> @item.出价</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3" style="background-color: #dff0d8; ">


                        <div class="text1 alert-success" style="display:block; white-space: nowrap;"> @item.隐藏游戏账号</div>

                    </div>

                    <div class="col-xs-3 col-sm-3 col-md-3 " style="background-color: #dff0d8; ">


                        <div class="text1 alert-success" style="display:block; white-space: nowrap;"> @item.隐藏qq</div>

                    </div>





                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1" style="display:block; white-space: nowrap;">备注</div>

                    </div>


                    <div class="col-xs-9 col-sm-9 col-md-9" style="">


                        <div class="text1" style="display:block; white-space: nowrap;">@item.备注 &nbsp;</div>

                    </div>




                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1" style="display:block; white-space: nowrap;">已经报价人数</div>

                    </div>

                    <div class="col-xs-9 col-sm-9 col-md-9" style="">


                        <div class="text1" style="display:block; white-space: nowrap;">？</div>

                    </div>




                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1" style="display:block; white-space: nowrap;">发布日期</div>

                    </div>
                    <div class="col-xs-9 col-sm-9 col-md-9" style="">


                        <div class="text1" style="display:block; white-space: nowrap;">@item.日期</div>

                    </div>



                    <div class="col-xs-3 col-sm-3 col-md-3" style="  ">


                        <div class="text1" style="display:block; white-space: nowrap;">需求编号</div>

                    </div>
                    <div class="col-xs-9 col-sm-9 col-md-9" style="">


                        <div class="text1" style="display:block; white-space: nowrap;">@item.需求编号</div>

                    </div>


                </div>


            </div>

        </a>


    }







    <div>&nbsp;</div>
    <div>&nbsp;</div>
    <div>&nbsp;</div>
    <div>&nbsp;</div>
    <div>&nbsp;</div>
    <div>&nbsp;</div>





    <footer class="footer navbar-fixed-bottom" style="text-align:center;">


        <div class="container" style="text-align:center;  ">



            <div class="row" style="text-align:center">











                <a href="/yuanshen/发布需求">
                    <div class="col-xs-12 col-sm-12 col-md-12" style="background-color: white; ">

                        <span class="glyphicon glyphicon-plus-sign" aria-hidden="true" style="font-size: 30px;width: 80px;
                padding: 10px;
                background-size: cover;
                border-radius: 25px;"></span>
                        <div class="text1" style="display:block; white-space: nowrap;"> 发布需求</div>

                    </div>
                </a>








            </div>



        </div>



    </footer>


</body>
</html>
